<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Description>Biblioteca de funções para Cliente Api do Framework da Codout</Description>
    <Copyright>Codout</Copyright>
    <Company>Clovis Coli Jr &amp; Marcelo Paiva</Company>
    <Authors>Clovis Coli Jr &amp; Marcelo Paiva</Authors>
    <PackageLicenseUrl>https://github.com/Codout/Codout.Framework/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Codout/Codout.Framework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Codout/Codout.Framework</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Codout</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>1.0.2</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Codout.Framework.Api.Dto\Codout.Framework.Api.Dto.csproj" />
  </ItemGroup>

</Project>
