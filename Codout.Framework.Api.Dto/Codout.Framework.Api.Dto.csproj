<Project Sdk="Microsoft.NET.Sdk" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Clovis Coli Jr &amp; Marcelo Paiva</Authors>
    <Company>Clovis Coli Jr &amp; Marcelo Paiva</Company>
    <Description>Biblioteca de funções para classes DTO da Api do Framework da Codout</Description>
    <Copyright>Codout</Copyright>
    <PackageLicenseUrl>https://github.com/Codout/Codout.Framework/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Codout/Codout.Framework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Codout/Codout.Framework</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Codout</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

</Project>
